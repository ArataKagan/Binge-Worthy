  "license": "ISC",
  "devDependencies": {
    "nodemon": "^1.19.4",
    "request": "^2.88.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "c": "^1.1.1",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "eslint": "^7.30.0",
    "express": "^4.16.4",
    "express-validator": "^6.12.0",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "sequelize": "^6.6.4",
    "twilio": "^3.65.0"
  }
